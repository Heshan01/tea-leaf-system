{
  "expo": {
    "name": "Tea Admin",
    "slug": "tea-admin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-admin.png",
    "scheme": "teaadmin",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icon-admin.png",
      "bundleIdentifier": "com.heshan.teaadmin",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Tea Admin app needs your location to view and manage trip locations.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Tea Admin app needs your location to support tracking features."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-admin.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.heshan.teaadmin",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icon-admin.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow Tea Admin to access your location while you use the app.",
          "locationAlwaysAndWhenInUsePermission": "Allow Tea Admin to access your location for tracking features."
        }
      ],
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4603e61a-668c-410c-b546-eb674c3f2f87"
      }
    },
    "owner": "heshan0001"
  }
}
